{
  "family": "cloudstak-api",
  "executionRoleArn": "arn:aws:iam::202533506302:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "cloudstak-api-container",
      "image": "202533506302.dkr.ecr.us-east-1.amazonaws.com/ecr-cloudstak-api:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": ["subnet-00f00d18def29693c"],
      "securityGroups": ["sg-04b97bb3708241dc5"],
      "assignPublicIp": "ENABLED"
    }
  }
}
